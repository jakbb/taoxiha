tbname='log_crawl_baidu_app'result=`/Applications/MAMP/Library/bin/mysql -P8889 -uroot -hlocalhost -pzhang <<EOFuse taoxiha;SHOW TABLES LIKE '%$tbname';EOF`result=`echo $result|awk '{print $3}'` if [ "$result" != "$tbname" ] then/Applications/MAMP/Library/bin/mysql -P8889 -uroot -hlocalhost -pzhang <<EOFuse taoxiha;set names utf8;CREATE TABLE log_crawl_baidu_app( tasknum varchar(100) DEFAULT NULL COMMENT '任务编号',sourcenum varchar(100) DEFAULT NULL COMMENT '资源编号',docid int(10) DEFAULT NULL COMMENT '文档ID',appname varchar(100) DEFAULT NULL COMMENT '应用名称',icoimg varchar(100) DEFAULT NULL COMMENT '图片地址',downurl varchar(100) DEFAULT NULL COMMENT '下载地址',appsize varchar(100) DEFAULT NULL COMMENT '应用大小',market varchar(100) DEFAULT NULL COMMENT '来源市场',apptype varchar(100) DEFAULT NULL COMMENT '应用类型',version varchar(100) DEFAULT NULL COMMENT '应用版本',updatetime varchar(100) DEFAULT NULL COMMENT '更新时间',downsize varchar(100) DEFAULT NULL COMMENT '下载次数',othermarket varchar(2000) DEFAULT NULL COMMENT '其他市场') ENGINE=InnoDB DEFAULT CHARSET=utf8  COMMENT='百度应用集合'; quit EOFecho 'success'elseecho 'exist the table 'fi